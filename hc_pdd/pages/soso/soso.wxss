page {
    width: 100%;
    height: 100%;
}

.toubu {
    width: 100%;
    height: 444rpx;
    position: fixed;
    display: flex;
    align-items: center;
    justify-content: center;
    top: 0;
}

.toubu_img {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
}

button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    background: none;
    line-height: normal;
}

button::after {
    content: none;
}

.soso {
    width: 80%;
    height: 85rpx;
    border-radius: 50rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 137rpx;
    position: relative;
}

.sosobut {
    width: 617rpx;
    height: 85rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

.soso_img {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    top: 20rpx;
    left: 50rpx;
}

.soso text {
    color: #333333;
    font-size: 22rpx;
}

